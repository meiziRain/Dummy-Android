<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_conversation">

    <fragment
        android:id="@+id/navigation_conversation"
        android:name="com.meizi.dummy.ui.conversation.ConversationFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_conversation" />

    <fragment
        android:id="@+id/navigation_contact"
        android:name="com.meizi.dummy.ui.contact.ContactFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_contact" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.meizi.dummy.ui.profile.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>